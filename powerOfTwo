import java.util.Scanner;
public class Power_Of_Two {
    public static boolean powOfTwo(int n)
    {
        if(n<=0)
       {
        return false;
       }
       return (n&(n-1))==0;
    }
    public static void main(String[]args)
    {
        Scanner sc =new Scanner(System.in);
        int n=sc.nextInt();
        System.out.print(powOfTwo(n));
    }
    
}

